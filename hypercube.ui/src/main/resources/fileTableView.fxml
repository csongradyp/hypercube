<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.control.TableView" VBox.vgrow="ALWAYS" stylesheets="style/fileTableView.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columns>
        <TableColumn fx:id="fileNameColumn" editable="false" minWidth="100.0" prefWidth="200.0" text="Name"/>
        <TableColumn fx:id="extColumn" editable="false" maxWidth="80.0" minWidth="45.0" prefWidth="55.0" text="Ext."/>
        <TableColumn fx:id="fileSizeColumn" editable="false" maxWidth="100.0" minWidth="60.0" prefWidth="60.0" text="Size"/>
        <TableColumn fx:id="dateColumn" editable="false" maxWidth="200.0" minWidth="110.0" prefWidth="115.0" text="Date"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</fx:root>
