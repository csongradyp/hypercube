<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean parent="daoCollection" class="org.springframework.beans.factory.config.ListFactoryBean">
        <property name="sourceList">
            <list merge="true">
                <bean class="com.noe.hypercube.googledrive.dao.DriveFileEntryDao" />
                <bean class="com.noe.hypercube.googledrive.dao.DriveMappingDao" />
            </list>
        </property>
    </bean>

</beans>
